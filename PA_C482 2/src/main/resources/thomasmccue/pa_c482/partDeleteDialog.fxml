<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="100.0" prefWidth="409.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thomasmccue.pa_c482.PartDeleteDialogController">
   <children>
      <Label fx:id="confirmationDialog" alignment="CENTER" contentDisplay="CENTER" layoutY="29.0" prefHeight="17.0" prefWidth="409.0" text="Are you sure you would like to delete this?" />
      <Label fx:id="partToBeDeleted" alignment="CENTER" layoutX="2.0" layoutY="71.0" prefHeight="26.0" prefWidth="405.0" textAlignment="CENTER" />
      <ButtonBar layoutX="205.0" layoutY="112.0" prefHeight="41.0" prefWidth="193.0">
        <buttons>
          <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteClicked" prefHeight="24.0" prefWidth="114.0" text="Delete" />
            <Button fx:id="cancelButton" layoutX="140.0" layoutY="17.0" mnemonicParsing="false" onAction="#onCancelClicked" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
